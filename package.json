{
  "name": "book-api-learning-lab",
  "version": "1.0.0",
  "description": "A training API project covering REST, SOAP, JWT auth, headers, query/path params, status codes, Postman testing, and Kubernetes deployment.",
  "main": "src/app.js",
  "type": "module",
  "scripts": {
    "start": "node src/app.js",
    "dev": "nodemon src/app.js",
    "db:sync": "node -e \"import('./src/db.js').then(m=>m.sequelize.sync({alter:true}))\""
  },
  "keywords": [
    "express",
    "soap",
    "rest-api",
    "nodejs",
    "jwt",
    "postgres",
    "sequelize",
    "kubernetes",
    "postman"
  ],
  "author": "rpamu",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.20.2",
    "express": "^4.19.2",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.13.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.1",
    "soap": "^0.45.0"
  },
  "devDependencies": {
    "nodemon": "^3.1.0"
  }
}
